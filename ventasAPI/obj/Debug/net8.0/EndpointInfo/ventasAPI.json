  "components": {
    "schemas": {
      "CustomerDTO": {
        "required": [
          "bornDate",
          "document",
          "email",
          "gender",
          "lastName",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "minLength": 1,
            "type": "string"
          },
          "bornDate": {
            "type": "string",
            "format": "date-time"
          },
          "email": {
            "maxLength": 80,
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "document": {
            "type": "integer",
            "format": "int64"
          },
          "gender": {
            "$ref": "#/components/schemas/Gender"
          }
        },
        "additionalProperties": false
      },
      "Gender": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "InvoiceDTO": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "code": {
            "type": "string",
            "format": "uuid"
          },
          "sellerId": {
            "type": "integer",
            "format": "int32"
          },
          "customerId": {
            "type": "integer",
            "format": "int32"
          },
          "invoiceDetailsDto": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/InvoiceDetailDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InvoiceDetailDTO": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "quantify": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ProductDTO": {
        "required": [
          "details",
          "expiryDate",
          "imageURL",
          "manufacturingDate",
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "details": {
            "minLength": 1,
            "type": "string"
          },
          "manufacturingDate": {
            "type": "string",
            "format": "date-time"
          },
          "expiryDate": {
            "type": "string",
            "format": "date-time"
          },
          "imageURL": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SellerDTO": {
        "required": [
          "bornDate",
